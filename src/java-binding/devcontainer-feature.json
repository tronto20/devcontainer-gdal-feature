{
    "name": "Install GDAL With Java Binding.",
    "id": "java-binding",
    "version": "0.3.0",
    "description": "Build GDAL with JAVA Binding.",
    "options": {
        "gdalVersion": {
            "type": "string",
            "default": "3.9.1",
            "description": "GDAL Version."
        },
        "cpu": {
            "type": "string",
            "default": "4",
            "description": "Set CPU Count for build GDAL."
        },
        "jdk": {
            "type": "string",
            "default": "21",
            "description": "JDK Version."
        }
    },
    "containerEnv": {
        "LD_LIBRARY_PATH": "/usr/lib/x86_64-linux-gnu/jni"
    },
    "installsAfter": [
        "ghcr.io/devcontainers/features/common-utils"
    ]
}
